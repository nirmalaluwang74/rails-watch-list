<%= simple_form_for([@list, @bookmark]) do |f| %>
  
  <div class="form-inputs">
    <%= f.input :comment %>
  </div>

  <div class="form-inputs">
  <%= f.association :movie %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>

<p><%= link_to 'Back to movie lists', root_path %></p>


